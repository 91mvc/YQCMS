<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
<meta content="text/html; charset=iso-8859-9" http-equiv="Content-Type">
<title>后台管理系统</title>
<link href="@Url.Content("~/Content"+@ViewBag.CI.WebPath+"css/AdminSite.css")" rel="stylesheet" type="text/css" />
<script src="@Url.Content("~/Scripts/jquery-1.5.1.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/modernizr-1.7.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Content" + @ViewBag.CI.WebPath + "Category.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Content" + @ViewBag.CI.WebPath + "Cms.js")" type="text/javascript"></script>
@RenderSection("Css", false)
</head>
<body>
<div id="container">
    <div id="header">
      <div id="logo">
        <h1><span><a href="###">YQCMS后台管理</a></span></h1>
        <p></p>
      </div>
      <div id="userBox">
        @Html.Partial("_LogOnPartial")
      </div><!-- end of #userBox -->
      <div id="menu">
      @RenderSection("MenuInfo", false)
        <p id="rightLink"><a target=_blank href="@(ViewBag.CI.WebPath)">View Site</a></p>
      </div><!-- end of #menu -->
      
  </div><!-- end of  #header -->
      <hr>
@RenderBody()
  <!-- end of #content -->
<br class="clearFix">
</div><!-- end of #container -->
<hr>
<div id="footerWrap">
  <div id="footer">
      <div id="footRight">
        <div id="footStats">
        YQCMS Build 1.0
        </div>
        Copyright 2012<br>
          <span>@ViewBag.CI.Weburl</span>
      </div>
  </div>
  <br class="clearFix">
</div>
</body>
@RenderSection("Js", false)
</html>